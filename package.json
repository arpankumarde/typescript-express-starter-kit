{
  "name": "typescript-express-starter-kit",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "build": "npm run prisma:generate && tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate --no-hints",
    "preinstall": "node -v",
    "postinstall": "npm run prisma:generate"
  },
  "dependencies": {
    "@prisma/client": "6.18.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.7.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.18.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
